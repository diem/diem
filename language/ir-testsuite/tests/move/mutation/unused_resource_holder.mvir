    import 0x1.Diem;

    resource T{g: Diem.Diem<XUS.XUS>}

    public new(g: Diem.Diem<XUS.XUS>): Self.T {
        return T{g: move(g)};
    }

    public destroy(a: Self.T) {
        let c: Diem.Diem<XUS.XUS>;
        T{g: c} = move(a);
        Diem.destroy_zero<XUS.XUS>(move(c));
        return;
    }
}

