import 0x1.CoreAddresses;
import 0x1.Signer;
//! sender: diemroot
main(account: signer) {
    assert(Signer.address_of(&account) == CoreAddresses.DIEM_ROOT_ADDRESS(), 0);
    return;
}

//! new-transaction
import 0x1.CoreAddresses;
import 0x1.Signer;
main(account: signer) {
    assert(Signer.address_of(&account) != CoreAddresses.DIEM_ROOT_ADDRESS(), 1);
    return;
}

//! new-transaction
//! account: alice
//! sender: alice
import 0x1.CoreAddresses;
import 0x1.Signer;
main(account: signer) {
    assert(Signer.address_of(&account) != CoreAddresses.DIEM_ROOT_ADDRESS(), 2);
    assert({{alice}} != CoreAddresses.DIEM_ROOT_ADDRESS(), 3);
    return;
}
