module Test {
    resource T { b: bool }

    public new(): Self.T {
        return T { b: true };
    }

    public test(i: &u64, x: Self.T): u64 * Self.T * bool {
        return *move(i), move(x), false, 0x0;
    }

    public destroy(x: Self.T) {
        let b: bool;
        T { b } = move(x);
        return;
    }
}
