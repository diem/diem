// check: POSITIVE_STACK_SIZE_AT_BLOCK_END

module Test {
    public foo(v: u64): u64 * u64 {
        let one_less: u64;
        let one_more: u64;
        one_less = copy(v) - 1;
        one_more = move(v) + 1;
        return move(one_less), move(one_more);
    }

    public bar(): u64 * u64 * u64 {
        return 500, 8, 256;
    }
}

//! new-transaction
import {{default}}.Test;

main() {
    let y: u64;
    let x0: u64;
    let x1 : u64;
    x0, x1 = Test.foo(Test.bar());
    assert(move(x0) == 8, 42);
    assert(move(x1) == 256, 42);
    return;
}
