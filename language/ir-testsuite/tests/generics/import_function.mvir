module M {
    resource R { f: bool }

    public foo<T>(x: T) {
        abort 0;
    }

    public bar<T1, T2: resource, T3: unrestricted>(x: T3, y: T2, z: T1) {
        abort 0;
    }

    public r(): Self.R {
        return R { f: false };
    }
}

