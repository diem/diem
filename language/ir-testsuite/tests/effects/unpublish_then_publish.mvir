module M {
    import 0x1.Signer;

    resource R { x: bool }

    public init(s: &signer) {
        move_to<R>(move(s), R { x: false });
        return;
    }

    public test(s: &signer) acquires R {
        let r: Self.R;
        r = move_from<R>(Signer.address_of(copy(s)));
        move_to<R>(move(s), move(r));
        return;
    }
}
// check: EXECUTED


//! new-transaction
import {{default}}.M;

main(s: &signer) {
    M.init(move(s));
    return;
}
// check: EXECUTED


