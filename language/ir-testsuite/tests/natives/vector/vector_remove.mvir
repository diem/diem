//! new-transaction
import 0x0.Vector;
// remove on out-of-bounds index should fail
main() {
  let vec: vector<u64>;

  vec = Vector.empty<u64>();
  Vector.push_back<u64>(&mut vec, 0);
  _ = Vector.remove<u64>(&mut vec, 1);

  return;
}

// check: ABORTED
// check: Some(10)
