module A {
    struct T{f: u64}

    public new(f: u64): Self.T {
        return T{f: move(f)};
    }

    public t(this: &mut Self.T) {
        let f: &mut u64;
        f = &mut copy(this).f;
        *copy(f) = 2;
        assert(*move(f) == 2, 42);
        _ = move(this);
        return;
    }
}

//! new-transaction

import {{default}}.A;

main() {
    let x: A.T;
    let x_ref: &mut A.T;
    x = A.new(0);
    x_ref = &mut x;
    A.t(move(x_ref));
    return;
}
