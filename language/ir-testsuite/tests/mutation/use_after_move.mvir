module B {
    struct T{g: u64}

    public new(g: u64): Self.T {
        return T { g: move(g) };
    }
}


//! new-transaction
module A {
    import Transaction.B;
    struct T{value: B.T}
    public new(m: B.T): Self.T {
        return T{value: move(m)};
    }

    public t(this: &mut Self.T) {
        let ref1: &mut B.T;
        let ref2: &mut B.T;
        let b2: B.T;
        let x: B.T;
        ref1 = &mut move(this).value;
        ref2 = copy(ref1);
        b2 = B.new(3);
        *move(ref1) = move(b2);

        x = *move(ref2);

        return;
    }
}
