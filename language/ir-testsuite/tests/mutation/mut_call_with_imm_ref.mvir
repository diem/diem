module Token {
    resource T{value: u64}
    public new(m: u64): Self.T {
        return T{value: copy(m)};
    }
    public destroy(t: Self.T) {
        let value: u64;
        T {value} = move(t);
        return;
    }

    public read_value(this: &Self.T): u64 {
        let val: &u64;
        val = &copy(this).value;
        Self.bump_value(move(this));
        return *move(val);
    }

    public bump_value(this: &mut Self.T) {
        let val: &mut u64;
        let x: u64;
        val = &mut move(this).value;
        x = *copy(val) + 1;
        *move(val) = copy(x);
        return;
    }
}
// check: CALL_TYPE_MISMATCH_ERROR
