module A {
import 0x0.LBR;
    import 0x0.Libra;
    resource A { c: Libra.Libra<LBR.LBR> }
    public new(c: Libra.Libra<LBR.LBR>): Self.A {
        return A { c: move(c) };
    }
    public destroy_a(a: Self.A) {
        let c: Libra.Libra<LBR.LBR>;
        A { c } = move(a);
        Libra.destroy_zero<LBR.LBR>(move(c));
        return;
    }
}

