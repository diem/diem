//! account: alice, 0
//! account: bob, 0

//! new-transaction
//! sender: blessed
import 0x1.LBR;
import 0x1.LibraAccount;
import 0x1.Libra;
main(blessed: &signer) {
    LibraAccount.deposit<LBR.LBR>(copy(blessed), {{alice}}, LBR.mint(copy(blessed), 10000));
    LibraAccount.deposit<LBR.LBR>(copy(blessed), {{bob}}, LBR.mint(move(blessed), 10000));
    return;
}

//! new-transaction
//! sender: alice
//! gas-price: 1
main() {
    return;
}


//! new-transaction
//! sender: bob
//! gas-price: 1
main() {
    let x: u64;
    x = 1 + 2 + 3;
    return;
}


//! new-transaction
import 0x1.LibraAccount;
import 0x1.LBR;

main() {
    // Alice did less work than bob so she should pay less gas.
    assert(LibraAccount.balance<LBR.LBR>({{bob}}) < LibraAccount.balance<LBR.LBR>({{alice}}), 42);
    return;
}
