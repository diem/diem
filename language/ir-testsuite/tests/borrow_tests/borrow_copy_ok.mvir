module B {
    struct T {g: u64}

    public new(g: u64): Self.T {
        return T{g: move(g)};
    }

    public t(this: &Self.T) {
        let g: &u64;
        let y: u64;
        g = &copy(this).g;
        y = *move(g);
        _ = move(this);
        return;
    }
}

//! new-transaction

import {{default}}.B;

main() {
    let x: B.T;
    let y: &B.T;
    x = B.new(5);
    y = &x;
    B.t(move(y));
    return;
}
