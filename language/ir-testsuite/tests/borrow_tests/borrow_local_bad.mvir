module A {
    struct T{v: u64}

    public new(u: u64) : Self.T {
        return T{v: move(u)};
    }

    public value(this: Self.T): u64 {
        let f: &u64;
        f = &(&mut this).v;
        return *move(f);
    }
}

//! new-transaction

import {{default}}.A;
main() {
    let x: A.T;
    let x_ref: u64;
    x = A.new(5);
    x_ref = A.value(move(x));
    assert(move(x_ref) == 5, 42);
    return;
}
