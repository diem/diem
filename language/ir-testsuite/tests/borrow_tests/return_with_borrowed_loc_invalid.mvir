module M3 {
    struct X { y: Self.Y }
    struct Y { u: u64 }
    t3(): &u64 {
        let s: Self.X;
        let x: &Self.X;
        let y: &Self.Y;
        let u: &u64;
        s = X { y: Y { u: 0 } };
        x = &s;
        y = &move(x).y;
        u = &move(y).u;
        return move(u);
    }
}
