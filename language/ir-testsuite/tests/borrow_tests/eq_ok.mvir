module Tester {
    eqtest1() {
        let x: u64;
        let r: &mut u64;

        x = 0;
        r = &mut x;
        _ = freeze(copy(r)) == freeze(copy(r));
        return;
    }

    eqtest2() {
        let x: u64;
        let r: &mut u64;

        x = 0;
        r = &mut x;
        _ = freeze(copy(r)) == freeze(move(r));
        return;
    }

    neqtest1() {
        let x: u64;
        let r: &mut u64;

        x = 0;
        r = &mut x;
        _ = freeze(copy(r)) != freeze(copy(r));
        return;
    }

    neqtest2() {
        let x: u64;
        let r: &mut u64;

        x = 0;
        r = &mut x;
        _ = freeze(copy(r)) != freeze(move(r));
        return;
    }
}
