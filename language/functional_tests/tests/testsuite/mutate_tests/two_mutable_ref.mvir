module M {
  struct Foo {
      a: u64,
      b: u64,
  }
  public create_mutable_field_addresses(addr: &mut Self.Foo) {
      let a_ref: &mut u64;
      let b_ref: &mut u64;
      a_ref = &mut copy(addr).a;
      b_ref = &mut copy(addr).b;
      _ = move(a_ref);
      _ = move(b_ref);
      _ = move(addr);
      return;
  }
}
