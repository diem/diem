module M {
  struct Foo {
      a: u64,
      b: u64,
  }
  public create_mutable_field_addresses(addr: &mut V#Self.Foo) {
      let a_ref: &mut u64;
      let b_ref: &mut u64;
      a_ref = &mut copy(addr).a;
      b_ref = &mut copy(addr).b;
      release(move(a_ref));
      release(move(b_ref));
      release(move(addr));
      return;
  }
}
