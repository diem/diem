module A {
    resource T{v: u64}

    public A0(addr: address) acquires T {
        let x: &mut Self.T;
        let y: &mut Self.T;
        x = borrow_global<T>(get_txn_sender());
        y = borrow_global<T>(move(addr));
        release(move(x));
        release(move(y));
        return;
    }
}

