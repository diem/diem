module A {
    struct T{v: u64}

    public new(u: u64) : V#Self.T {
        return T{u: move(u)};
    }

    public value(this: V#Self.T): u64 {
        let f: &u64;
        f = &(&mut this).v;
        return *move(f);
    }
}

