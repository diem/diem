module A {
    resource T1{v: u64}

    public test(addr: address) acquires T1 {
        let x: &mut Self.T1;
        x = borrow_global_mut<T1>(get_txn_sender());
        _ = move(x);
        Self.acquires_t1();

        return;
    }

    acquires_t1() acquires T1 {
        let v: u64;
        T1 { v } = move_from<T1>(get_txn_sender());
        return;
    }

}
