module Test {
    resource T { i: u64, b: bool }

    public new_t(): Self.T {
        return T { i: 0, b: false };
    }

    public unpack_t(t: Self.T) {
        let i: u64;
        let flag: bool;
        T { i, b: flag } = move(t);
        return;
    }

}

//! new-transaction
import {{default}}.Test;
main() {
    let t: Test.T;

    t = Test.new_t();
    Test.unpack_t(move(t));

    return;
}
