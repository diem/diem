//! no-run: verifier, runtime

module M {
    resource Foo<T> { x: T }

    foo() {
        let x: Self.Foo<u64>;
        let y: u64;

        x = Foo<u64> { x: 42 };
        Foo<u64> { y } = move(x);
    }
}

