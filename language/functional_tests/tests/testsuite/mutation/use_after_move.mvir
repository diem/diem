import Transaction.B;
main() {
    let b: V#B.T;
    let a: V#A.T;
    let a_ref: &mut V#A.T;
    b = B.new(1);
    a = A.new(move(b));
    a_ref = &mut a;
    A.t(move(a_ref));

    return;
