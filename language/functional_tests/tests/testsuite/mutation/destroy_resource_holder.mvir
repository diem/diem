module A {
    import 0x0.LibraCoin;
    resource A { c: R#LibraCoin.T }
    public new(c: R#LibraCoin.T): R#Self.A {
        return A { c: move(c) };
    }
    public destroy_a(a: R#Self.A) {
        let c: R#LibraCoin.T;
        A { c } = move(a);
        LibraCoin.destroy_zero(move(c));
        return;
    }
}

