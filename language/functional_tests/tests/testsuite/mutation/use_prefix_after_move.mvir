module B {
    struct T{g: u64}

    public new(g: u64): V#Self.T {
        return T{g: move(g)};
    }

    public t(this: &mut V#Self.T) {
        let g: &mut u64;
        g = &mut move(this).g;
        *move(g) = 3;
        return;
    }
}

//! new-transaction

module A {
    import Transaction.B;
    struct T{f: V#B.T}

    public new(f: V#B.T): V#Self.T {
        return T{f: move(f)};
    }

    public t(this: &mut V#Self.T) {
        let ref1: &mut V#B.T;
        let ok: V#B.T;
        ref1 = &mut move(this).f;
        B.t(copy(ref1));
        ok = *move(ref1);
        return;
    }
}

