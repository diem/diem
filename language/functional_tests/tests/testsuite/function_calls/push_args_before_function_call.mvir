module A {
    public push_u64(): u64  {
        return 42;
    }

    public push_bool(): bool {
        return true;
    }

    public foo(u: u64, b: bool): u64 {
        if (move(b)) { return move(u) + 1;}
        else { return 0;}
    }
}

//! new-transaction
