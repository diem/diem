// check: NEGATIVE_STACK_SIZE_WITHIN_BLOCK

module Test {
    resource T { }

    public new(): Self.T {
        return T { };
    }

    public test(i: &u64, x: Self.T): u64 * Self.T * bool {
        return *move(i), move(x), false;
    }

    public destroy(x: Self.T) {
        T { } = move(x);
        return;
    }
}

//! new-transaction

import {{default}}.Test;

main() {
    let i: u64;
    let t: Test.T;
    let a: u64;
    let b: bool;
    let x: Test.T;
    let z: address;

    i = 0;
    t = Test.new();
    a, x, b, z = Test.test(&i, move(t));
    assert(move(a) == 0, 42);
    Test.destroy(move(x));
    assert(!move(b), 43);

    return;
}
