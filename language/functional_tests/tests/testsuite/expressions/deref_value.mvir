module A {
    struct T{f: u64}

    public new(f: u64): Self.T {
        return T{f: move(f)};
    }

    public t(this: &Self.T) {
      let f: &u64;
      let y: u64;
      f = &copy(this).f;
      y = *move(f);
      assert(copy(y) == 2, 42);
      _ = move(this);
      return;
    }
}

//! new-transaction

import {{default}}.A;

main() {
    let x: A.T;
    let x_ref: &A.T;

    x = A.new(2);
    x_ref = &x;
    A.t(move(x_ref));
    return;
}
