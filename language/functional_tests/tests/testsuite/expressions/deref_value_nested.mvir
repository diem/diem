import Transaction.A;
import Transaction.B;
main() {
    let b: V#B.T;
    let x: V#A.T;
    let x_ref: &V#A.T;

    b = B.new(2);
    x = A.new(move(b));
    x_ref = &x;
    A.t(move(x_ref));
    return;
}