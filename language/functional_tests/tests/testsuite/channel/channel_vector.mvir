//! new-transaction
module VecTest{
    import 0x0.Vector;

    resource VecBox {
        grid: Vector.T<bool>,
    }

    public test_move_vec_resource() {
        let t: Self.VecBox;
        let grid: Vector.T<bool>;

        grid = Vector.empty<bool>();

        t = VecBox {
            grid: move(grid),
        };

        move_to_sender_channel<VecBox>(move(t));

        return;
    }
}

